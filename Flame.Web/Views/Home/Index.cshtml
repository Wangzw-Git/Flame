@model IEnumerable<Flame.Data.TimerScheduler>

@{
    ViewBag.Title = "列表页";
}

<h2>Index</h2>
<p>
    @Html.ActionLink("新增", "Edit")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ts_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ts_NameSpace)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ts_RunStatu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ts_Interval)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ts_Desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ts_AddDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ts_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ts_NameSpace)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ts_RunStatu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ts_Interval)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ts_Desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ts_AddDate)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.Ts_Id }) |
            @Html.ActionLink("详情", "Details", new { id=item.Ts_Id }) |
            @Html.ActionLink("删除", "Delete", new { id=item.Ts_Id })
        </td>
    </tr>
}

</table>
